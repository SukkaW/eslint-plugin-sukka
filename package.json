{
  "name": "eslint-plugin-sukka",
  "version": "9.0.0",
  "description": "SukkaW extended ESLint rules",
  "repository": {
    "type": "git",
    "url": "https://github.com/SukkaW/eslint-plugin-sukka"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs",
      "default": "./dist/index.cjs"
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "lint": "eslint --format=sukka .",
    "build": "bunchee --no-sourcemap --minify"
  },
  "keywords": [
    "eslint",
    "eslint-plugin",
    "sukka",
    "sukkaw",
    "eslint-plugin-sukka",
    "eslint-config-sukka"
  ],
  "author": "SukkaW <https://skk.moe>",
  "license": "GPL-3.0-only",
  "devDependencies": {
    "bunchee": "^6.9.4",
    "eslint": "^9.39.2",
    "eslint-config-sukka": "^8.6.1",
    "eslint-formatter-sukka": "^8.6.1",
    "typescript": "^5.9.3"
  },
  "packageManager": "pnpm@10.28.2"
}
