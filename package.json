{
  "name": "eslint-plugin-sukka",
  "version": "9.0.0-beta.1",
  "description": "SukkaW extended ESLint rules",
  "repository": {
    "type": "git",
    "url": "https://github.com/SukkaW/eslint-plugin-sukka"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs",
      "default": "./dist/index.cjs"
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "lint": "eslint --format=sukka .",
    "build": "bunchee --no-sourcemap --minify",
    "test": "SWC_NODE_PROJECT=./tsconfig.test.json mocha --require @swc-node/register ./src/**/*.test.ts --timeout 5000",
    "prerelease": "pnpm run lint && pnpm run test && pnpm run build",
    "release": "bumpp -r --all --commit \"release: %s\" --tag \"%s\""
  },
  "keywords": [
    "eslint",
    "eslint-plugin",
    "sukka",
    "sukkaw",
    "eslint-plugin-sukka",
    "eslint-config-sukka"
  ],
  "author": "SukkaW <https://skk.moe>",
  "license": "GPL-3.0-only",
  "dependencies": {
    "@typescript-eslint/type-utils": "^8.54.0",
    "@typescript-eslint/types": "^8.54.0",
    "@typescript-eslint/utils": "^8.54.0",
    "foxts": "^5.2.1"
  },
  "devDependencies": {
    "@swc-node/register": "^1.11.1",
    "@swc/core": "^1.15.11",
    "@types/mocha": "^10.0.10",
    "@types/node": "^24.10.9",
    "@typescript-eslint/rule-tester": "^8.54.0",
    "bumpp": "^10.4.0",
    "bunchee": "^6.9.4",
    "eslint": "^9.39.2",
    "eslint-config-sukka": "^8.6.1",
    "eslint-formatter-sukka": "^8.6.1",
    "expect": "^30.2.0",
    "mocha": "^11.7.5",
    "ts-dedent": "^2.2.0",
    "typescript": "^5.9.3"
  },
  "peerDependencies": {
    "eslint": "*",
    "typescript": "*"
  },
  "packageManager": "pnpm@10.28.2"
}
